#demo_content
	.mainContentWrap
		// ============ Titles ============ //
		.demoAppWrap
			.demoAppName Application
			.demoAppDescr Description
			.demoDetails Links
			.demoInteract Interact
			.demoDeployWrap &nbsp;
		
		
		// ============ Example02 ============ //
		.demoAppWrap
			.demoAppName Example02
			.demoAppDescr Store two integers named A and B. Subtract from one and add to the other.
			.demoDetails
				a(href="https://github.com/hyperledger/fabric/blob/master/examples/chaincode/go/chaincode_example02/chaincode_example02.go" target="_blank").underline Chaincode
			.demoInteract 
				a(href="#").underline.showActions Show Actions
			.demoDeployWrap
				button(type="button" disabled="disabled" cc="example02").deployButton.bx--btn &nbsp;Deploy&nbsp;
				#example02Loading.loadingdiv.demoLoading
					span
					span
					span
					span
					span
					div
			.demoActionWrap.example02
				.demoCCSelectWrap
					span.smallerFont Select the correct chaincode:
					select(name="demo_chaincode_name" cc="example02").selectCC
						option(disabled="disabled" selected="selected") invalid
				.demoActionSelectWrap
					span.smallerFont Select an action:
					select(cc="example02").selectAction
						option(disabled="disabled" value="invokeA2B").demoAction#invokeA2B Transfer from A to B
						option(disabled="disabled" value="invokeB2A").demoAction#invokeB2A Transfer from B to A
						option(disabled="disabled" selected="selected" value="queryA").demoAction#queryA Query A
						option(disabled="disabled" value="queryB").demoAction#queryB Query B
				.demoActionButtonWrap
					span.smallerFont &nbsp;&nbsp;&nbsp;
					button(type="button" disabled="disabled" cc="example02").bx--btn--secondary.executeAction Execute

		
		
