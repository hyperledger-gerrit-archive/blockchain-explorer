		br

		// ============ Log Panel ============ //
		div(style="text-align:right;").logControls
			label.apiDetailsCheck
				input(type="checkbox" name="showAPIdetails" style="vertical-align: top;")
				span Show API details&nbsp;
		#logWrapper.logControls
			svg(title="Clear Logs")#clearLogs
				use(xlink:href="/img/icons/sprite.svg#common--close")
			#demoLogs

		// ============ Notification Panel ============ //
		br
		.demoNotifications
			.notificationWrap.notifcationSuccess.deployHelperText
				svg.notificationImg.notifcationSuccess
					use(xlink:href="/img/icons/sprite.svg#notifications--success")
				.notificationBody
					strong Demo Chaincode successfully deployed:
					div Now you can hit an action button below "Actions" to invoke a chaincode function.
				svg.notificationClose
					use(xlink:href="/img/icons/sprite.svg#common--close")
			.notificationWrap.invokeHelperText
				svg.notificationImg
					use(xlink:href="/img/icons/sprite.svg#support--info-stroke-circle")
				.notificationBody
					strong Your invocation was sent:
					div You can check out API details in the panel below.  Now click a Query button to query chaincode state.
				svg.notificationClose
					use(xlink:href="/img/icons/sprite.svg#common--close")
			.notificationWrap.queryHelperText
				svg.notificationImg
					use(xlink:href="/img/icons/sprite.svg#support--info-stroke-circle")
				.notificationBody
					strong Your query was sent:
					div You can check out API details in the panel below.
				svg.notificationClose
					use(xlink:href="/img/icons/sprite.svg#common--close")
