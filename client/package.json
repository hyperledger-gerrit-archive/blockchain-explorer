{
  "name": "hyperledger-explorer-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aphrodite": "^1.2.5",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.29",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^4.4.8",
    "react-refetch": "^1.0.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-select": "^1.2.1",
    "recharts": "^1.0.0-beta.9",
    "redux": "^3.7.2",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.2",
    "typeface-roboto": "0.0.50"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:8080/",
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.2.1",
    "jest-enzyme": "^4.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.5.1",
    "sinon": "^4.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{test,js,jsx}",
      "!src/**/*.{json}",
      "!src/config.js",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/registerServiceWorker.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      },
      "./src/components/": {
        "branches": 0,
        "statements": 0
      },
      "./src/store/reducers/**/*.js": {
        "statements": 0
      }
    },
    "snapshotSerializers": []
  }
}
